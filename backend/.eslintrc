{
  "extends": [
    "airbnb-base"
  ],
  "rules": {
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ],
    "operator-linebreak": [
      "error",
      "none"
    ],
    "linebreak-style": 0,
    "quotes": [
      2,
      "double",
      {
        "avoidEscape": true
      }
    ]
  }
}